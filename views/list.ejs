<%- include("header")  -%>
    <div class="box" id="heading">
    <h1><%= listTitle %> </h1>
    </div>

<div class="box">
     <%   for(var i=0; i<dd.length ; i++){  %>
        <div class="item">
        <input type="checkbox" >
           <p> <%= dd[i] %> </p>
        </div>
      <%  } %>
    
   <form class="item" action="/" method="post">
    <input type="text" name="newitem" placeholder="new Item" autocomplete="off">
    <button type="submit" name="named" value="<%= listTitle %>">+</button>
   </form>

</div>
<%- include("footer") -%>